{
    "execute": {
        "use_dask": true,
        "n_workers": 4,
        "memory_limit": 4000000000
    },
    "component": {
        "framework": "ARL",
        "name": "predict_vislist"
    },
    "logging": {
        "filename": "../../../test_results/gleam-pipeline.log",
        "filemode": "a",
        "format": "%(asctime)s,%(msecs)d %(name)s %(levelname)s %(message)s",
        "datefmt": "%H:%M:%S",
        "level": "INFO"
    },
    "inputs": {
        "skymodel": "../../../test_results/gleam_skymodel.hdf",
        "vis_list": "../../../test_results/gleam_empty_vislist.hdf"
    },
    "outputs": {
        "vis_list": "../../../test_results/gleam_perfect_vislist.hdf"
    },
    "imaging": {
        "context": "wstack",
        "vis_slices": 11
    },
    "applypb_skymodel": true
}